<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lasu.hyperduty.mapper.SysDictDataMapper">

    <select id="selectByDictTypeId" resultType="com.lasu.hyperduty.entity.SysDictData">
        SELECT * FROM sys_dict_data WHERE dict_type_id = #{dictTypeId} ORDER BY dict_sort ASC
    </select>

</mapper>